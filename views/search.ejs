<div class = "author">
    <h1 class = "author__heading">
        SEARCH RESULTS
    </h1>
</div>

<section class = "articles">
    <ul class="article-ul">
        <!--Post-->
        <% data.forEach(post => { %>
      <li>
          <a href = "/post/<%= post._id %>">
              <span>
                <%= post.title %>
              </span>
              <span class = "article-list__date">
                <%= post.createdAt %>
              </span>
          </a>
      </li> 
      <% }) %>
      </ul>
</section>